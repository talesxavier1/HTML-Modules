{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch 2",
            "port": 9222,
            "request": "launch",
            "type": "chrome",
            "url": "http://127.0.0.1:8080",
            "webRoot": "${workspaceFolder}",
            "protocol": "inspector",
            "runtimeArgs": [
                "--harmony",
                "--no-deprecation"
            ],
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}"
            ]
        }
    ]
}