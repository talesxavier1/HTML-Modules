{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "CodeDiagram",
            "port": 9222,
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8080/",
            "webRoot": "${workspaceFolder}/CodeDiagram",
            "runtimeArgs": [
                "--harmony",
                "--no-deprecation"
            ],
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/CodeDiagram"
            ],
            "skipFiles": [
                "<anonymous>"
            ]
        },
        {
            "name": "JsonSchemaEditor",
            "port": 9222,
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}/JsonSchemaEditor",
            "outFiles": [
                "${workspaceFolder}/JsonSchemaEditor"
            ],
        }
    ]
}